  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thingsSDK/flasher.js/issues"
  },
  "homepage": "https://github.com/thingsSDK/flasher.js#readme",
  "dependencies": {
    "bunyan": "^1.8.0",
    "node-binary": "^1.1.0",
    "request": "^2.72.0",
    "serialport": "^2.0.7-beta1",
    "tar.gz": "^1.0.3",
    "unzip": "^0.1.11"
  },
  "devDependencies": {
    "chai": "^3.5.0",
